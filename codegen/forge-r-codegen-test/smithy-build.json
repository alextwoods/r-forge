{
  "version": "1.0",
  "projections": {
    "Crc": {
      "transforms": [
        {
          "name": "includeServices",
          "args": { "services":  ["forge.crt#Crc"]}
        }
      ],
      "plugins": {
        "r-forge-codegen": {
          "service": "forge.crt#Crc",
          "module": "crc"
        }
      }
    },
    "Common": {
      "transforms": [
        {
          "name": "includeServices",
          "args": { "services":  ["forge.crt#Common"]}
        }
      ],
      "plugins": {
        "r-forge-codegen": {
          "service": "forge.crt#Common",
          "module": "common"
        }
      }
    }
  }
}